<h1>DogeBit</h1>


<article>
<table border="1" style="width:100%;" id="magictable" >
  <tr>
    <th>Long URL</th> 
    <th>Shortened URL</th>
    <th>Number of times Visited</th>
  </tr>
  <% if @uri %>
  <% @uri.each do |x| %>
  	<tr>
    	<td><a href="<%=x.long %>" target="_blank"><%=x.long%></a></td>
    	<td><a href="/<%=x.short %>" target="_blank">http://localhost:9393/<%=x.short%></a></td> 
    	<td><%=x.counter%></td>
    </tr>
  <% end %>
</table>
</article>

<% end %>


